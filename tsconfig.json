{
	"display": "Node 14",
	"extends": "@tsconfig/node14/tsconfig.json",

	"compilerOptions": {
		"module": "CommonJs",
		"target": "es6",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"outDir": "dist",
		"baseUrl": "./TsModules",
		"paths": {
			"mysql-migrations": [ "mysql-migrations.d.ts" ],
			"ckey": [ "ckey.d.ts" ],
			"crypto": [ "crypto.d.ts" ]
		}
	},

	"lib": ["es2015"],

	"exclude": [
		"src/tests"
	],
}